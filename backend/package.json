{
  "name": "smart-clinic-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc -p .",
    "vercel-build": "npm run prisma:gen && npm run build",
